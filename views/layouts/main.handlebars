<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>Move Mate</title>
    {{! Bootstrap }}
    <link
      href='https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css'
      rel='stylesheet'
      integrity='sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9'
      crossorigin='anonymous'
    />
    {{! Map }}
    <link rel='stylesheet' href='/css/pricing.css' />
    {{! <link rel="stylesheet" href="/css/pricing.css"> }}
    <link rel='stylesheet' href='/css/reset.css' />
    <link rel='stylesheet' href='/css/home.css' />
    <link rel='stylesheet' href='/css/jass.css' />
    <link rel='stylesheet' href='/css/style.css' />
  </head>
  <body>
    <nav
      class='navbar navbar-expand-lg bg-blue position-static pt-3 pb-3 border-bottom border-warning'
    >
      <div class='container-fluid'>
       
        <a href='/' class="navigation navigation-center p-3{{#if logged_in}} d-none{{/if}}"><span
            class='color-yellow'
          >MOVE</span>MATE</a>
        <a class='navbar-brand' href='#'>{{#if logged_in}}<span
              class='color-yellow spacing'
            >{{name}}</span>{{/if}}</a>
        <button
          class='navbar-toggler border border-warning'
          type='button'
          data-bs-toggle='offcanvas'
          data-bs-target='#offcanvasScrolling'
          aria-controls='navbarNav'
          aria-expanded='false'
          aria-label='Toggle navigation'
        >
          <span class='navbar-toggler-icon'></span>
        </button>
        <div
          class='offcanvas offcanvas-start custom-offcanvas border border-start-0 border-warning'
          style='width: 320px;'
          data-bs-scroll='true'
          data-bs-backdrop='false'
          tabindex='-5'
          id='offcanvasScrolling'
          aria-labelledby='offcanvasScrollingLabel'
        >
          <div class='offcanvas-header'>
            <h5 class='offcanvas-title' id='offcanvasScrollingLabel'><span
                class='color-yellow'
              >MOVE</span>MATE</h5>
            <button
              type='button'
              class='btn-close text-reset'
              data-bs-dismiss='offcanvas'
              aria-label='Close'
            ></button>
          </div>
          <div class='offcanvas-body d-flex justify-content-lg-center bg-blue'>

            <ul class='navbar-nav'>
              <li class='nav-item ml-5 mr-5'>
                <a
                  class='nav-link active navigation navigation-center'
                  aria-current='page'
                  href='/'
                >HOME</a>
              </li>
              <li class='nav-item ml-5 mr-5'>
                <a
                  class='nav-link text-white navigation navigation-center'
                  href='#about'
                >ABOUT</a>
              </li>
              {{#if logged_in}}
                <li class='nav-item ml-5 mr-5'>
                  <a
                    class='nav-link color-yellow navigation navigation-center'
                    href='/pricing'
                  >PRICING</a>
                </li>
              {{/if}}

              {{#if logged_in}}
                <li class='nav-item ml-5 mr-5'>
                  <a
                    class='nav-link text-white navigation navigation-center'
                    href='/faq'
                  >FAQ</a></li>
                <li class='nav-item ml-5 mr-5'><a
                    href='/'
                    class='nav-link text-white navigation navigation-center'
                    id='logout'
                  >LOGOUT</a></li>

              {{else}}
                <li class='nav-item ml-5 mr-5'>
                  <a
                    class='nav-link text-white navigation navigation-center'
                    href='/login'
                  >FAQ</a></li>
                <li class='nav-item ml-5 mr-5'><a
                    class='nav-link text-white navigation navigation-center'
                    id='login'
                    href='/login'
                  >LOGIN</a></li>

                <li class='nav-item ml-5 mr-5'>
                  <a
                    class='nav-link text-white navigation navigation-center'
                    id='signup'
                    href='/signup'
                  >SIGNUP</a></li>

              {{/if}}

            </ul>
          </div>
        </div>
      </div>
    </nav>

    <main>

      <!-- Render the sub layout -->
      {{{body}}}
    </main>
    <footer>
      <div class='card border-0 bg-blue'>
        <div class='card-body d-flex flex-row'>
          <section class='position-relative col-1 pt-4 d-none d-md-block'>
            <div class='ml-5 mt-3 card bg-body border-0' style='width: 5rem;'>
              <img
                src='/images/logo.jpg'
                class='card-img-top p-1 rounded card-shadow'
                alt='...'
              />

            </div>
          </section>
          
          <section class='col-12 col-md-10 pt-5 d-flex flex-row justify-content-evenly navigation navigation-center' style="font-size:200%!important;letter-spacing:1.5rem">
            
            <a href='/'><span
              class='color-yellow'
            >MOVE</span>MATE</a>
            
          </section>
          <div class='col-12 text-end text-white'>
            <p class='m-0 fs-6'>Copyright Â© MoveMate, All Right Reserved</p>
          </div>
        </div>
      </div>
    </footer>

